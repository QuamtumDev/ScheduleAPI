{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/",
      "methods": ["GET"]
    },
    {
      "src": "/now",
      "dest": "/now/",
      "methods": ["GET"]
    },
    {
      "src": "/next",
      "dest": "/next/",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET"]
    }
  ]
  },
  "version": 2
}
